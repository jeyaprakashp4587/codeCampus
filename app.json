{
  "expo": {
    "name": "codeCampus",
    "slug": "codecampus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/appss.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeyaprakash4587.codeCampus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jeyaprakash4587.codeCampus"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "511e8cb2-ad8b-496e-a2fe-f1a5792df82b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "jeyaprakash4587",
    "plugins": [
      "expo-router"
    ],
    "updates": {
      "url": "https://u.expo.dev/511e8cb2-ad8b-496e-a2fe-f1a5792df82b"
    }
  }
}
